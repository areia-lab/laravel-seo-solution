{
  "name": "areia-lab/laravel-seo-solution",
  "description": "SEO management package with Tailwind UI backend, OpenGraph & Twitter, and Blade helpers.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Akramul Hoque",
      "email": "akramulhoque.engineer@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "AreiaLab\\LaravelSeoSolution\\": "src/"
    }
